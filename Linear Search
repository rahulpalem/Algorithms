list=[]
flag=0
n=int(input("enter no of elements:"))
x=int(input("enter value to be searched:"))
j=n-1
for i in range(0,n):
    ele=int(input())
    list.append(ele)
for i in range(0,int(j/2)+1):
    if(list[i]==x):
        flag=1
        print("Element is found at position:",i)
        break
    if(list[j]==x):
        flag=1
        print("Element is found at position:",j)
        break 
    j=j-1    
if(flag==0):
    print("Element not found")
